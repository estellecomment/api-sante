{
  "name": "FastAPI CSV",
  "description": "FastAPI CSV",
  "logo": "https://fastapi.tiangolo.com/img/icon-white.svg",
  "repository": "https://github.com/betagouv/fastapi-csv",
  "website": "https://github.com/jrieke/fastapi-csv",
  "env": {
    "CSV_PATH": {
      "description": "The URL to the CSV data file that you want to serve by your API.",
      "value": "https://raw.githubusercontent.com/jrieke/fastapi-csv/main/people.csv",
      "required": true
    },
    "DELIMITER": {
      "description": "If your CSV file is not comma-separated, specify the delimiter character (for example '|')",
      "value": ",",
      "required": false
    }
  },
  "formation": {
    "web": {
      "amount": 1,
      "size": "S"
    }
  }
}